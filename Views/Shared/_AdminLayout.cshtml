<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Mawjoud Admin Template</title>

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon" href="~/AdminStyle/images/favicon.png">
    <link rel="shortcut icon" href="~/AdminStyle/images/favicon.png">
   
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/normalize.css@8.0.0/normalize.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/font-awesome@4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/lykmapipo/themify-icons@0.1.2/css/themify-icons.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/pixeden-stroke-7-icon@1.2.3/pe-icon-7-stroke/dist/pe-icon-7-stroke.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/flag-icon-css/3.2.0/css/flag-icon.min.css">

    <link href="~/AdminStyle/css/style.css" rel="stylesheet" />


</head>
<body>

    <aside id="left-panel" class="left-panel">
        <nav class="navbar navbar-expand-sm navbar-default">
            <div id="main-menu" class="main-menu collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="active">
                        <a href="~/Admin/Dashboard"><i class="menu-icon fa fa-laptop"></i>Dashboard </a>
                    </li>
                    <li>
                        <a href="~/Admin/Member">  <i class="menu-icon fa fa-table"></i>Members</a>
                    </li>
                    <li>
                        <a href="~/Admin/Categories">  <i class="menu-icon fa fa-table"></i>Categories</a>
                    </li>

                    <li>
                        <a href="~/Admin/Product">  <i class="menu-icon fa fa-table"></i>Products</a>
                    </li>




                </ul>
            </div><!-- /.navbar-collapse -->
        </nav>
    </aside>
    <div id="right-panel" class="right-panel">
        <!-- Header-->
        <header id="header" class="header">

            <div class="top-left">
                <div class="navbar-header">

                    <a class="navbar-brand" href="./"><img src="~/AdminStyle/images/logo.png" alt="Logo"></a>
                    <a class="navbar-brand hidden" href="./"><img src="~/AdminStyle/images/logo2.png" alt="Logo"></a>
                    <a id="menuToggle" class="menutoggle"><i class="fa fa-bars"></i></a>
                </div>
            </div>
            <div class="top-right">
                <div class="header-menu">
                    <div class="header-left">


                    </div>

                    <div class="user-area dropdown float-right">
                        <a href="#" class="dropdown-toggle active" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <img class="user-avatar rounded-circle" src="~/AdminStyle/images/admin.jpg" alt="User Avatar">
                        </a>

                        <div class="user-menu dropdown-menu">
                            <a class="nav-link" href="#"><i class="fa fa- user"></i>My Profile</a>

                            <a class="nav-link" href="#"><i class="fa fa- user"></i>Notifications <span class="count">13</span></a>

                            <a class="nav-link" href="#"><i class="fa fa -cog"></i>Settings</a>

                            <a class="nav-link" href="#"><i class="fa fa-power -off"></i>Logout</a>
                        </div>
                    </div>

                </div>
            </div>
        </header>
        <!-- /#header -->
        <div>
            @RenderBody()

        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/jquery@2.2.4/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.4/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/js/bootstrap.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery-match-height@0.7.2/dist/jquery.matchHeight.min.js"></script>
    <script src="~/AdminStyle/js/main.js"></script>
    <script src="~/AdminStyle/js/lib/data-table/datatables.min.js"></script>
    <script src="~/AdminStyle/js/lib/data-table/dataTables.bootstrap.min.js"></script>
    <script src="~/AdminStyle/js/lib/data-table/dataTables.buttons.min.js"></script>
    <script src="~/AdminStyle/js/lib/data-table/buttons.bootstrap.min.js"></script>
    <script src="~/AdminStyle/js/lib/data-table/jszip.min.js"></script>
    <script src="~/AdminStyle/js/lib/data-table/vfs_fonts.js"></script>
    <script src="~/AdminStyle/js/lib/data-table/buttons.html5.min.js"></script>
    <script src="~/AdminStyle/js/lib/data-table/buttons.print.min.js"></script>
    <script src="~/AdminStyle/js/lib/data-table/buttons.colVis.min.js"></script>
    <script src="~/AdminStyle/js/datatables-init.js"></script>


</body>
</html>
