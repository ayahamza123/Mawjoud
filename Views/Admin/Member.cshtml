@model IEnumerable<Mawjoud2.DAL.Members>

@{
    ViewBag.Title = "Member";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Member</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Active</th>
            <th>Email vertified</th>
            <th>Activation code</th>
            <th>Action</th>


        </tr>
    </thead>
    <tbody>



        @foreach (var item in Model)
        {
            <tr>
      
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
     

                <td>
                    @Html.DisplayFor(modelItem => item.IsActive)
                </td>
               
                <td>
                    @Html.DisplayFor(modelItem => item.IsEmailVerified)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ActivationCode)
                </td>
                <td>
                    @Html.ActionLink("Edit", "MemberEdit", new { memberId = item.MemberId}) |
                 
                    @Html.ActionLink("Delete", "MemberDelete", new { memberId = item.MemberId})
                </td>
            </tr>
        }
    </tbody>

</table>
